# Copy this file to .env.local and fill in your actual values

# API URL for accessing your backend
API_URL=your_api_url

# Optional: App Version (automatically set during tag deployments)
APP_VERSION=0.0.1

# Firebase Configuration

# Firebase API Key
FIREBASE_API_KEY=your_firebase_api_key_here

# Firebase Auth Domain (usually your-project-id.firebaseapp.com)
FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com

# Firebase Project ID
FIREBASE_PROJECT_ID=your-project-id

# Firebase Storage Bucket (usually your-project-id.appspot.com)
FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com

# Firebase Messaging Sender ID
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id

# Firebase App ID
FIREBASE_APP_ID=your_firebase_app_id

# Firebase Analytics Measurement ID (optional)
FIREBASE_MEASUREMENT_ID=your_measurement_id
